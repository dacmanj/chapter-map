<h1>Listing chapters</h1>

<% #= gmaps4rails(@json) %>


<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Website</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Email 1</th>
    <th>Ein</th>
    <th></th>
  </tr>

<% @chapters.each do |chapter| %>
  <tr>
    <td><%= link_to chapter.name, edit_chapter_path(chapter) %></td>
    <td><%= link_to 'Website', chapter.website %></td>
    <td><%= chapter.street %></td>
    <td><%= chapter.city %></td>
    <td><%= chapter.state %></td>
    <td><%= chapter.zip %></td>
    <td><%= chapter.email_1 %></td>
    <td><%= chapter.ein %></td>
    <td><%= link_to content_tag(:i, "", class: "icon-trash icon-red") + "", chapter, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger remove_fields" %> </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Chapter', new_chapter_path %>

<h2>Import Chapters</h2>

<%= form_tag import_chapters_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>


