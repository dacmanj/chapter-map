<%= simple_nested_form_for(@member) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contituent_id if admin_user? %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :chapter_code if admin_user?  %>

    <%= render "chapter_leaders", :f => f %>
    <%= f.input :spouse_first_name %>
    <%= f.input :spouse_last_name %>
    <%= f.input :address, :as => :text, :input_html => { :cols => 40, :rows => 3 } %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <% if has_role? :admin %>
    <%= f.input :import_id %>
    <%= f.input :address_import_id %>
    <%= f.input :phone_import_id %>
    <%= f.input :email_import_id %>
    <% end %>
    <%= f.input :suppress_from_directory %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn btn-primary" %>
  </div>
<% end %>
